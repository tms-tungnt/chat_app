<% if current_user %>
  <%= image_tag current_user.avata_url %>
  <h2>
    <%= current_user.full_name %>
  </h2>
  <%= link_to "Edit_account", edit_user_registration_path %>
  <%= button_to "Log out", destroy_user_session_path, data: {turbo: 'false'}, method: :delete %>
<% else %>
  <%= link_to "login", new_user_session_path %>
  <%= link_to "Create_account", new_user_registration_path %>
<% end %>
